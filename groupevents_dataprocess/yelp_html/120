<script type="text/javascript">
								var fbConnector = new yelp.ext.FBConnectMechatron({
				'serviceID': 16,
				'fbConnectRegisterURL': "/facebook_connect/register",
				'fbConnectImportUserInfoURL': "/user_photos/import",
				'fromSignup': false
			});
			yelp.page.set('fbPhotoConnector', fbConnector);

			fbConnector.nextURL = "/import_photo_fb/next";
			fbConnector.signupTrackURL = "/track_metric/7?signup_source=MEXICAN_CATEGORY_SIGNUP";
			fbConnector.importPhotoClickTrackURL = "/track_metric/12?signup_source=MEXICAN_CATEGORY_SIGNUP";
			fbConnector.sharingPermissionGivenTrackURL = "/track_metric/13?signup_source=MEXICAN_CATEGORY_SIGNUP";

			yelp.event.domLoaded(function() {
				$('fb-loader-container').hide();
				$('fb-import-photo').observe('click', function(e) {
					fbConnector.importFBPhoto('fb-photo-container', 'fb-action-container', 'fb-loader-container');
				});
				var swfObject = window.SWFObject ? new SWFObject() : null;
				if (swfObject === null || swfObject.installedVer.major == 0) {
					$("fb-no-flash").show();
				}
			});


</script>
			</div>



